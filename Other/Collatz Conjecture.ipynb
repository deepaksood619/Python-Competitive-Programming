    "def collatz(n):\n",
    "    print(n, end=' ')\n",
    "    if n == 1:\n",
    "        return\n",
    "    elif n%2 == 0:\n",
    "        collatz(n//2)\n",
    "    else:\n",
    "        collatz(n*3 + 1)"
